{
  "ConnectionStrings": {
    "Default": "PRODUCTION_CONNECTION_STRING"
  },
  "FileStorage": {
    "AzureBlobStorage": {
      "ConnectionString": "PRODUCTION_AZURE_STORAGE_CONNECTION_STRING",
      "ContainerName": "plateaumedehr"
    }
  },
  "App": {
    "ServerRootAddress": "https://{TENANCY_NAME}.api.plateaumed.com/",
    "ClientRootAddress": "https://{TENANCY_NAME}.app.plateaumed.com/",
    "CorsOrigins": "https://app.plateaumed.com,https://*.app.plateaumed.com"
  },
  "Sentry": {
    "Dsn": "PRODUCTION_SENTRY_DSN",
    "MaxRequestBodySize": "Always",
    "SendDefaultPii": true,
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Warning",
    "AttachStackTrace": true,
    "Debug": true,
    "DiagnosticsLevel": "Error"
  },
  "Snowstorm": {
    "BaseUrl": "PRODUCTION_SNOWSTORM_BASEURL"
  }
}