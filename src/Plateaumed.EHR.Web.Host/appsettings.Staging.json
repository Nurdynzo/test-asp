{
  "ConnectionStrings": {
    "Default": "STAGING_CONNECTION_STRING"
  },
  "FileStorage": {
    "AzureBlobStorage": {
      "ConnectionString": "STAGING_AZURE_STORAGE_CONNECTION_STRING",
      "ContainerName": "plateaumedehr"
    }
  },

  "App": {
    "ServerRootAddress": "https://{TENANCY_NAME}.api.plateaumed-staging.com/",
    "ClientRootAddress": "https://{TENANCY_NAME}.app.plateaumed-staging.com/",
    "CorsOrigins": "https://app.plateaumed-staging.com,https://*.app.plateaumed-staging.com"
  },
  "Sentry": {
    "Dsn": "STAGING_SENTRY_DSN",
    "MaxRequestBodySize": "Always",
    "SendDefaultPii": true,
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Warning",
    "AttachStackTrace": true,
    "Debug": true,
    "DiagnosticsLevel": "Error"
  },
  "Snowstorm": {
    "BaseUrl": "STAGING_SNOWSTORM_BASEURL"
  }
}
