{
  "ConnectionStrings": {
    "Default": "TEST_CONNECTION_STRING"
  },
  "FileStorage": {
    "AzureBlobStorage": {
      "ConnectionString": "TEST_AZURE_STORAGE_CONNECTION_STRING",
      "ContainerName": "plateaumedehr"
    }
  },
  "App": {
    "ServerRootAddress": "https://{TENANCY_NAME}.api.plateaumed-dev.com/",
    "ClientRootAddress": "https://{TENANCY_NAME}.app.plateaumed-dev.com/",
    "CorsOrigins": "https://app.plateaumed-dev.com,https://*.app.plateaumed-dev.com,http://localhost:4200"
  },
  "Sentry": {
    "Dsn": "TEST_SENTRY_DSN",
    "MaxRequestBodySize": "Always",
    "SendDefaultPii": true,
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Warning",
    "AttachStackTrace": true,
    "Debug": true,
    "DiagnosticsLevel": "Error"
  },
  "Snowstorm": {
    "BaseUrl": "TEST_SNOWSTORM_BASEURL"
  }
}
